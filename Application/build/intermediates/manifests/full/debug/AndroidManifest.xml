<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.customchoicelist"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="4"
        android:targetSdkVersion="22" />

    <!-- Min/target SDK versions (<uses-sdk>) managed by build.gradle -->
    <application
        android:allowBackup="true"
        android:icon="@drawable/seshadriswami"
        android:label="@string/app_name"
        android:theme="@style/Theme.NoTitle" >
        <activity android:name="com.example.android.customchoicelist.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />

                <uses-permission android:name="android.permission.WAKE_LOCK" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.android.customchoicelist.Activity_1"
            android:label="@string/title_activity_1"
            android:theme="@style/Theme.NoTitle" >
        </activity>
        <activity
            android:name="com.example.android.customchoicelist.Activity_2"
            android:label="@string/title_activity_2"
            android:theme="@style/Theme.NoTitle" >
        </activity>
    </application>

</manifest>